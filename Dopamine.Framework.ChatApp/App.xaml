<Application x:Class="Dopamine.ChatApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Dopamine.ChatApp"
             >
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
                <!--语言-->
				<ResourceDictionary x:Name="Language" Source="/Resources/zh-CN.xaml"/>
                
                <!--样式-->
				<ResourceDictionary x:Name="DictionaryResource_1" Source="/Styles/DictionaryResource_1.xaml"/>

			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
